{
  "name": "target_checkout",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "enzyme-adapter-react-16": "^1.1.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.7",
    "react-scripts": "1.0.17",
    "react-test-renderer": "^16.2.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "concurrently": "^3.5.1",
    "electron": "^1.8.4",
    "electron-builder": "^20.8.1",
    "electron-is-dev": "^0.3.0",
    "electron-packager": "^11.2.0",
    "enzyme": "^3.3.0",
    "gulp": "^3.9.1",
    "gulp-notify": "^3.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^6.4.0",
    "gulp-sass": "^3.1.0",
    "gulp-watch": "^4.3.11",
    "is-electron": "^2.1.0",
    "istanbul": "^0.4.5",
    "redux-immutable-state-invariant": "^2.1.0",
    "require-dir": "^1.0.0",
    "socket.io": "^2.1.0"
  },
  "scripts": {
    "start": "PORT=7000 BROWSER=none concurrently \"react-scripts start\" \"gulp watch\" ",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "scss": "gulp watch",
    "electron": "electron .",
    "electron-dev": "ELECTRON_START_URL=http://localhost:7000 electron .",
    "elec-test": "concurrently \"npm start\" \"npm run electron-dev\"",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --ignore=/src --ignore=/node_modules --ignore=/gulp --ignore=/public --ignore=/scss --ignore=coverage --ignore=gulpfile.js --out=release-builds",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/"
    ],
    "coverageReporters": [
      "lcov"
    ]
  },
  "build": {
    "appId": "com.example.app",
    "productName": "Target Checkout",
    "copyright": "Target CorpÂ©",
    "files": [
      "*.js",
      "build/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "category": "Checkout"
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ],
      "category": "Development"
    }
  }
}
